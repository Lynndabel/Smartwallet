# Morph Testnet Deployment Configuration
# ===========================================
# Copy this file to .env and fill in your actual values

# Your wallet private key (without 0x prefix)
# IMPORTANT: Never commit this to git! Keep it secret!
PRIVATE_KEY=your_private_key_here

# Morph Testnet RPC URL
RPC_URL=https://rpc-quicknode-holesky.morphl2.io

# Morph Testnet Chain ID
CHAIN_ID=2810

# Etherscan API Key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas settings (optional - foundry will estimate if not provided)
GAS_LIMIT=3000000
GAS_PRICE=1000000000

# Deployment settings
DEPLOYMENT_SALT=smartwallet_v1
